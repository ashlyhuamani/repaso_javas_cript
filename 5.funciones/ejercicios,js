// crear un funcion que reciba por parametro una lista y un alemento a encontrar retornar 
// si el elemento a encontrar retornar si el elemento fue encontrado o no 
function buscarElemento(lista, el) {
    let mensaje="kcapa"
    for (let i=0;i< lista.length;i++) {
        if (lista[i]== el) {
        mensaje="elemento encontrada"
      return mensaje// Elemento encontrado
    }
  }
  mensaje="elemento no encontrada"
  return mensaje // Elemento no encontrado
}

let amigos=["ashly","alex","angel","ana"]
console.log(buscarElemento(amigos,"kcapa"))


